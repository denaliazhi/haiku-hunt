<!DOCTYPE html>
<html lang="en">
  <%- include("./partials/head.ejs") %>
  <body>
    <%- include("./partials/navigation.ejs"); %>
    <main>
      <div class="page-header">
        <h1><%= title %></h1>
        <p>
          <%= entries.length %> <%= entries.length === 1 ? "result" :
          "results"%>
        </p>
      </div>
      <div class="card-container">
        <% entries.map(entry => { %> <%- include("./partials/landmark-card.ejs",
        {entry}) %> <% }) %>
      </div>
    </main>
  </body>
</html>
